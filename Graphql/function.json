{
    "scriptFile": "___init___.py",
    "bindings": [
      {
        "authLevel": "anonymous",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": [
          "get",
          "post",
          "put",
          "delete",
          "patch"
        ],
        "route": "{*path}"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "$return"
      }
    ]
  }